<div class="col-lg-8 progress rounded-round" style="margin-bottom: 20px">
                <div class="progress-bar bg-{{foo}}" style="width: {{ quizresult.note |round(2, 'floor') }}%">
                  <span>{{ quizresult.note |round(2, 'floor') }}</span>
                </div>
              </div>

                 </div>


{% if quizresult is not null %}

                                <div class="text-center">
                <h6 class="mb-0 font-weight-semibold">{{quizresult.dateend | date('d-m-Y  H:i ') }} </h6>
              </div>
              <div class="row col-lg-12">
                                {% if quizresult.note > 60 %}{% set foo = 'success' %} {% endif %}
